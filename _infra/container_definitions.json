[
  {
    "name": "main",
    "image": "${account_id}.dkr.ecr.${region}.amazonaws.com/${name}:${image_tag}",
    "cpu": 0,
    "memory": 128,
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "${region}",
        "awslogs-group": "/${name}/ecs",
        "awslogs-stream-prefix": "main"
      }
    }
  }
]
